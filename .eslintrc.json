{
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended"
    ],
    "plugins": ["vue"],
    "parserOptions": {
         "parser": "babel-eslint"
     },
    "env": {"browser": true},
    "globals": {},
    "rules": {
      "no-console": "off",
      "semi": "error",
      "indent": ["error", 2],
      "quotes": ["error", "single"]
    }
}
